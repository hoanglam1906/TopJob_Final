<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignupServlet</servlet-name>
        <servlet-class>controller.SignupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmProServlet</servlet-name>
        <servlet-class>controller.EmProServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManJobServlet</servlet-name>
        <servlet-class>controller.ManJobServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StuProServlet</servlet-name>
        <servlet-class>controller.StuProServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FindJobServlet</servlet-name>
        <servlet-class>controller.FindJobServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignupServlet</servlet-name>
        <url-pattern>/SignupServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmProServlet</servlet-name>
        <url-pattern>/EmProServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManJobServlet</servlet-name>
        <url-pattern>/ManJobServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StuProServlet</servlet-name>
        <url-pattern>/StuProServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FindJobServlet</servlet-name>
        <url-pattern>/FindJobServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            3000
        </session-timeout>
    </session-config>
</web-app>
